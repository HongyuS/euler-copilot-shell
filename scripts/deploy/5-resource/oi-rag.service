[Unit]
After=network.target nss-lookup.target
[Service]
User=root
WorkingDirectory=/usr/lib/euler-copilot-rag
Environment="PYTHONPATH=/usr/lib/euler-copilot-rag"
Environment="CONFIG=/etc/euler-copilot-rag/data_chain/env"
ExecStart=/usr/bin/python3 data_chain/apps/app.py
ExecReload=/bin/kill -HUP $MAINPID
Restart=on-failure
RestartSec=10
LimitNOFILE=infinity
[Install]
WantedBy=multi-user.target